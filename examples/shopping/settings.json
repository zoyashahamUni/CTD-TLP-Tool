{
  "factors": [
    {
      "name": "a_logout_after_add",
      "ltl": "F((step = add) & F((step = logout) & !end_of_test))"
    },
    {
      "name": "b_max_items",
      "values": [
        { "value": 3, "ltl": "F(items = 3) & !F(items = 4) & !F(items = 5)" },
        { "value": 4, "ltl": "F(items = 4) & !F(items = 5)" },
        { "value": 5, "ltl": "F(items = 5)" }
      ]
    },
    {
      "name": "c_removed_ever",
      "ltl": "F((step = remove) & (items > 0))"
    }
  ],
  "step_var": "step",
  "end_flag": "end_of_test",
  "test_rule": "TRUE"                   
}
